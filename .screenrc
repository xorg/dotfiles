# Tab bar

hardstatus off
hardstatus alwayslastline
hardstatus string "%{+b wk} %-w%{+b wk} %n %t %{-}%+w %41=%H @ %l %-= %= %D,%c:%s "
vbell off # Turns visual bell off

# Do not show startup message

startup_message off

attrcolor b ".I"
termcapinfo xterm 'Co#256:AB=\E[48;5;%dm:AF=\E38;5;%dm'
defbce "on"
term putty-256color
